{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier",
    "tslint-eslint-rules"
  ],
  "linterOptions": {
    "exclude": [
      "src/**/*.js"
    ]
  },
  "rules": {
    "jsx-no-multiline-js": false,
    "jsx-no-lambda": false,
    "no-var-requires": false,
    "arrow-return-shorthand": true,
    "align": [true, "parameters", "statements"],
    "ban": false,
    "class-name": true,
    "comment-format": [true, "check-space"],
    "curly": true,
    "eofline": false,
    "forin": true,
    "indent": [true, "spaces", 4],
    "interface-name": [true, "always-prefix"],
    "jsdoc-format": true,
    "label-position": true,
    "max-line-length": [true, 160],
    "member-access": true,
    "no-any": false,
    "no-arg": true,
    "no-bitwise": false,
    "no-console": [
      true,
      "error",
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ],
    "no-consecutive-blank-lines": true,
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-variable": true,
    "no-empty": true,
    "no-eval": true,
    "no-shadowed-variable": true,
    "no-string-literal": false,
    "no-switch-case-fall-through": true,
    "no-trailing-whitespace": false,
    "no-unused-expression": true,
    "object-literal-sort-keys": false,
    "one-line": [
      true,
      "check-catch",
      "check-else",
      "check-open-brace",
      "check-whitespace"
    ],
    "ordered-imports": false,
    "quotemark": [true, "single"],
    "radix": false,
    "semicolon": [true, "always", "ignore-bound-class-methods"],
    "switch-default": true,
    "trailing-comma": true,
    "triple-equals": true,
    "typedef": [true, "parameter", "property-declaration"],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-module",
      "check-operator",
      "check-separator",
      "check-type",
      "check-typecast"
    ],
    "no-duplicate-super": true,
    "no-misused-new": true,
    "max-statements": [
      2,
      13
    ],
    "complexity": [
      "error",
      {
        "max": 10
      }
    ],
    "max-nested-callbacks": [
      2,
      3
    ],
    "max-params": [
      "error",
      5
    ]
  },
  "no-use-before-declare": true,
  "no-sequences": 0,
  "react/display-name": 0,
  "react/no-unused-prop-types": 0,
  "react/jsx-key": 0,
  "react/no-unescaped-entities": 0,
  "eqeqeq": 2,
  "react/prop-types": 0,
  "jsx-quotes": 0,
  "no-proto": 0,
  "no-throw-literal": 0,
  "no-useless-escape": 0,
  "no-unused-vars": "error",
  "key-spacing": 0,
  "no-underscore-dangle": 0,
  "no-unused-expressions": 0,
  "no-shadow": 0,
  "no-shadow-restricted-names": 0,
  "no-extend-native": 0,
  "no-var": 2,
  "new-cap": 0,
  "semi-spacing": 0,
  "space-unary-ops": 0,
  "space-infix-ops": 0,
  "consistent-return": 0,
  "strict": 0,
  "camelcase": 0
}